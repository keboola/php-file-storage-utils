parameters:
    level: max
    checkMissingIterableValueType: false
    paths:
        - src
        - tests
    ignoreErrors:
        - "#Trying to invoke mixed but it's not a callable#"
        - '#Property Keboola\\FileStorage\\Path\\RelativePath\:\:\$path \(string\) on left side of \?\? is not nullable\.#'
    scanFiles:
      - tests/common/EchoLogger.php
